server:
  port: 8084

eureka:
  client:
    serviceUrl:
      defaultZone: http://localhost:7070/eureka/

spring:
  application:
    name: borrow-server

logging:
  level:
    org.springframework.cloud.sleuth: debug
  path: logs/
  file:
    max-size: 1GB

management:
  endpoints:
    web:
      exposure:
        include: '*'